@model IEnumerable<WebApplication2_2.Models.Store>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LatitudeOfStore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LongitudeOfStore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfStore)
        </th>
        <th></th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LatitudeOfStore)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LongitudeOfStore)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfStore)
        </td>
        <td>
            <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
            <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
            <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
        </td>
    </tr>
}
</table>
